{
  "prettier.requireConfig": true,
  "prettier.onlyUseLocalVersion": true,
  "suggest.maxCompleteItemCount": 10,
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "typescript", "elm", "markdown"],
  "coc.preferences.formatOnType": false,
  "eslint.run": "onSave",
  "tsserver.formatOnType": false,
  "coc.preferences.formatOnInsertLeave": false,
  "suggest.disableKind": true,
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmAnalyseTrigger": "change"
      }
    }
  },
  "snippets.userSnippetsDirectory": "~/.config/nvim/UltiSnips", 
  "snippets.extends": {
    "typescript": ["javascript"]
  }
}
